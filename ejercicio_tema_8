public class Main{
    
    public static void main(String[] args){
        Persona persona = new Persona(34,"Paula",221676299);
        System.out.printf("La edad de la persona es: " + persona.getEdad() + "\n");
        System.out.printf("El nombre de la persona es: " + persona.getNombre()+ "\n");
        System.out.printf("El telefono de la persona es: " + persona.getTelefono()+ "\n");
    }
}

class Persona{
    private int edad;
    private String nombre;
    private int telefono;
    
    public Persona(int edad, String nombre, int telefono){
        this.edad = edad;
        this.nombre = nombre;
        this.telefono = telefono;
    }

    public void setEdad(int edad) {
        this.edad = edad;
    }
    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public void setTelefono(int telefono) {
        this.telefono = telefono;
    }
    public int getEdad() {
        return this.edad;
    }

    public String getNombre() {
        return this.nombre;
    }

    public int getTelefono() {
        return this.telefono;
    }

}
